# This is an example of environment variables needed for the application.

# Ollama Configuration
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=gemma3:4b

# Vector Store Configuration
VECTOR_STORE_PATH=vectors/pubmed_vectors

# Data Configuration
CSV_PATH=data/cleaned_pubmed_papers.csv

# LangSmith 설정 (선택 사항)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=your_langchain_api_key_here
LANGCHAIN_PROJECT=depression-chatbot

# Tavily API 키 (웹 검색 기능에 필요)
TAVILY_API_KEY=your_tavily_api_key_here

# 로깅 설정
LOG_LEVEL=INFO

# API keys or configuration settings
API_KEY=your_api_key_here
BASE_URL=http://localhost:11434
MODEL_NAME=hf.co/HIHO999/gemma3-finetune-Depression-gguf:latest
TEMPERATURE=0.1